@model List<dotNettbankAdmin.Models.TransactionRow>



<div class="panel panel-default" data-id="Transactions">
    <div class="panel-heading">
        <h3>Transaksjoner</h3>
    </div>
    <!-- SØKE FELT-->
    <div id="custom-search-input">
        <div class="input-group col-md-12">
            <input type="text" id="search" class="form-control input-lg" placeholder="Søk blant transaksjoner..." />
            <span class="input-group-btn">
                <button class="btn btn-info btn-lg" type="button">
                    <i class="glyphicon glyphicon-search"></i>
                </button>
            </span>
        </div>
    </div>

    <div class="panel-body">
        <!-- TABLE -->
        <table class="table table-hover search-table paging-table">
            <thead class="thead-inverse">
                <tr>
                    <th>Fra Konto</th>
                    <th>Kunde</th>
                    <th>Til Konto</th>
                    <th>Kunde</th>
                    <th>Dato Betalt</th>
                    <th>Dato Registrert</th>
                    <th>Beløp</th>
                    <th>KID</th>
                </tr>
            </thead>
            <tbody>
               

                @foreach (var item in Model)
                {
                    <tr id="search-row" class="row-padding">
                        <td>@Html.DisplayFor(modelItem => item.FromAccountNo)</td>
                        <td>@Html.DisplayFor(modelItem => item.FromBirthNo)</td>
                        <td>@Html.DisplayFor(modelItem => item.ToAccountNo)</td>
                        <td>@Html.DisplayFor(modelItem => item.ToBirthNo)</td>
                        <td>@Html.DisplayFor(modelItem => item.DatePayed)</td>
                        <td>@Html.DisplayFor(modelItem => item.Date)</td>
                        <td>@Html.DisplayFor(modelItem => item.Amount)</td>
                        <td>@Html.DisplayFor(modelItem => item.Message)</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
<script src="~/Scripts/AdminPartials.js"></script>


