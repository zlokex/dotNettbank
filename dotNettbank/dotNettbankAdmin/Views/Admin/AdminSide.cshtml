
@model dotNettbankAdmin.Models.AdminSideModel

@{
    ViewBag.Title = "AdminSide";
}


<body class="hold-transition skin-black sidebar-mini">
    <div class="wrapper">
        <!-- Main Header -->
        <header class="main-header">
            <!-- Logo -->
            <a href="#" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b>HB</b>A</span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>HardBank</b>Admin</span>
            </a>
            <!-- Header Navbar -->
            <nav class="navbar navbar-static-top" role="navigation">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                <!-- Navbar Right Menu -->
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                       
                        <!-- User Account Menu -->
                        <li class="dropdown user user-menu">
                            <!-- Menu Toggle Button -->
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <!-- The user image in the navbar-->
                                <img  src="/Content/img/adminIMG.png" class="user-image" alt="User Image">
                                <!-- hidden-xs hides the username on small devices so only the image appears. -->
                                <span class="hidden-xs">@ViewBag.Email</span>
                            </a>
                           
                        </li>
                        <!-- Logg Ut Toggle Button -->
                        <li>
                            <a href="#" data-toggle="modal" data-target="#logOutModal">Logg ut</a>                      
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
        
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar scroll-wrapper">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- Sidebar user panel (optional) -->
                <div class="user-panel">
                    <div class="pull-left image">
                        <img src="/Content/img/adminIMG.png" class="img-circle" alt="User Image">
                    </div>
                    <div class="pull-left info">
                        <p>@ViewBag.UserName</p>
                    </div>
                </div>
                
                <!-- /.search form -->
                <!-- Sidebar Menu -->
                <ul class="sidebar-menu">
                    <li class="header">Bank oversikt</li>
                    <!-- Optionally, you can add icons to the links -->
                   

                    
                    <!-- Kunder -->
                    <li><a href="#" data-id="FindCustomers" class="getPartial"><i class="fa fa-user"></i> <span>Kunder</span></a></li>
                    <!-- Kontoer -->
                    <li><a href="#" data-id="Accounts" class="getPartial"><i class="fa fa-list"></i> <span>Kontoer</span></a></li>
                    <!-- Betalinger -->
                    <li><a href="#" data-id="RegBetaling" class="getPartial"><i class="fa fa-files-o"></i> <span>Betalinger</span></a></li>
                    <!-- Transaksjoner -->
                    <li><a href="#" data-id="Transactions"  class="getPartial"><i class="fa fa-credit-card"></i> <span>Transaksjoner</span></a></li>

                    <!--<li><a href="#"><i class="fa fa-user"></i> <span>Kunder</span></a></li>-->
                    <!--<li><a href="#" id="accounts_link"><i class="fa fa-list"></i> <span>Kontoer</span></a></li>-->
                    
                    <li class="header">Logg</li>
                    <li><a href="#" data-id="Audit" class="getPartial"><i class="fa fa-database"></i> <span>Database revisjon</span></a></li>
                </ul>

                <div id="tagsdiv">
                    <!-- /.sidebar-menu -->
                    <!-- Tab meny-->
                    <ul class="sidebar-menu">
                        <li class="header"><i class="fa fa-user"></i>  <span>Valgte kunder</span></li>
                    </ul>
                    <ul class="tagsmenu" id="customer-tags">
                        <!-- <li class="tag"><div class="tag"><p><i class="fa fa-times"></i> Bruks 929291928</p> </div></li>-->
                    </ul>
                    <ul class="sidebar-menu">
                        <li class="header"><i class="fa fa-list"></i>  <span>Valgte kontoer</span></li>
                    </ul>
                    <ul class="tagsmenu" id="account-tags">
                        <!-- <li class="tag"><div class="tag"><p><i class="fa fa-times"></i> Bruks 929291928</p> </div></li>-->
                    </ul>
                </div>
            </section>
            <!-- /.sidebar -->
        </aside>




        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
           
            <!-- Main content -->
            <section id="productsPlace" class="content">
                <!-- Your Page Content Here -->
                <h2 style="text-align:center">Velkommen til HardBank sin Admin side</h2>
                <p style="text-align:center">Velg et av panelene i menyen for å begynne</p>
            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
        <!-- Main Footer -->
        <footer class="main-footer">
            <!-- To the right -->
            <div class="pull-right hidden-xs">
             
            </div>
            <!-- Default to the left -->
            <strong>Copyright &copy; 2016 <a href="#">Kro Prouctions</a>.</strong> All rights reserved.
        </footer>
       
        <!-- /.control-sidebar -->
        <!-- Add the sidebar's background. This div must be placed
             immediately after the control sidebar -->
        
    </div>
</body>

<script src="~/Scripts/app.min.js"></script>
<script src="~/Scripts/AdminSide.js"></script>


<!--LOGG OUT MODAL-->
<div id="logOutModal" class="modal fade" role="dialog">
    <div class="modal-dialog modal-sm">

        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Logg ut</h4>
            </div>
            <div class="modal-body">
                <p>Er du sikker på at du vil logge ut?</p>
            </div>
            <div class="modal-footer">
                @Html.ActionLink("Ja", "Logout", "Admin", null, new { @class = "btn btn-dark-theme" })
                <button type="button" class="btn btn-dark-theme" data-dismiss="modal">Nei</button>
            </div>
        </div>
    </div>
</div>

